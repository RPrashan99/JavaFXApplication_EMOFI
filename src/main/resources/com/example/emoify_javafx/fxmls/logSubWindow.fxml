<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.emoify_javafx.controllers.logSubWindowController">
   <children>
      <VBox alignment="CENTER" prefHeight="350.0" prefWidth="500.0" spacing="10">
      
          <Button onAction="#handleRunPython" text="Run Python Script" />
          <TextArea fx:id="logArea" editable="false" prefHeight="350" prefWidth="580" wrapText="true" />
         <padding>
            <Insets top="5.0" />
         </padding>
      
      </VBox>
   </children>
</AnchorPane>
